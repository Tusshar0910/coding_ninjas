#include<iostream>
using namespace std;

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int n;
    cin >>n;
    int i = n;
    while (i >= 1) {
        int j = i;
        while (j >= 1){
             cout << i;
             j--;
        }
      cout << endl;
        i--;
    }
    return 0;
}
